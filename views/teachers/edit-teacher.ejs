<%- include('../includes/head.ejs') %>
</head>

<body>
    <%- include('../includes/navigation.ejs') %>
    <main>

        <div class=" container form-group col-md-5">
            <br>
            <h4>Edit teacher information</h4>
            <br>

            <form action="">
                <div class="form-group">
                    <input type="text" class="form-control" id="firstName" value="<%= teacher.firstName %>">
                </div>
                <div class="form-group">
                    <input type="text" class="form-control" id="lastName" value="<%= teacher.lastName %>">
                </div>
                <div class="form-group">
                    <input type="email" class="form-control" id="email" value="<%= teacher.email %>">
                </div>
                <div class="form-group">
                    <input type="text" class="form-control" id="userId" value="<%= teacher.userId %>">
                </div>
                <div class="form-group">
                    <input type="text" class="form-control" id="dept" value="<%= teacher.dept %>">
                </div>

                <button type="submit" class="btn btn-success">Update</button>
                <button type="submit" class="btn btn-danger">delete</button>
                <button  class="btn btn-primary">Cancel</button>

            </form>
        </div>



    </main>